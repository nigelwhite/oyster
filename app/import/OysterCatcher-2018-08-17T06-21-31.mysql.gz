INSERT INTO `cpn` VALUES ('3','','$(''img'').addClass(‘oyster1’);',''),('19','html {\r\n  font-family: ''helvetica neue'', helvetica, arial, sans-serif;\r\n}\r\n\r\n.inhere {\r\n  margin: auto;\r\n  width: 95%;\r\n  padding: 50px;\r\n}\r\n\r\ntable {\r\n  table-layout: fixed;\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  border: 3px solid grey;\r\n}\r\n\r\ntd,\r\nth {\r\n  border: solid 1px grey;\r\n}\r\n\r\ncaption {\r\n  padding: 20px;\r\n  font-style: italic;\r\n  caption-side: top;\r\n  text-align: left;\r\n  letter-spacing: 1px;\r\n  font-size: 1.25em;\r\n}\r\n\r\nth,\r\ntd {\r\n  text-align: center;\r\n}\r\n\r\ntd {\r\n  letter-spacing: 1px;\r\n}\r\n\r\nth tfoot {\r\n  letter-spacing: 2px;\r\n}\r\n\r\nthead th {\r\n  height: 70px;\r\n}\r\n\r\ntbody tr,\r\ntfoot tr {\r\n  height: 50px;\r\n}\r\n\r\nthead th {\r\n  border: 3px solid grey;\r\n}\r\n\r\n.y {\r\n  background-color: aquamarine;\r\n}','',''),('274','table .profiles {table-layout: fixed;\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  border: 3px solid purple;\r\n}\r\n\r\n.profiles td {\r\n    border: none;\r\n}\r\n\r\n.profiles th[scope="row"] {border: none;}\r\n\r\n.profiles tbody {text-align: center;\r\n  }\r\n\r\n.profiles td {\r\n  letter-spacing: 1px;\r\n}\r\n\r\n.profiles th tfoot{\r\n  letter-spacing: 2px;\r\n}\r\n\r\n.profiles thead th, tfoot td {\r\n  font-family: ''Rock Salt'', cursive;\r\n}\r\n\r\n. profiles tbody td {\r\n  height: 40px;\r\n}','','');
